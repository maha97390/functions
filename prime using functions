#include<stdio.h>
#include<conio.h>
void main()
{
    int a;
    printf("Enter a number:");
    scanf("%d",&a);
    prime(a);
}
void prime(int x)
{
    int count;
    for(int i=2;i<x;i++)
    {
      if(x%i==0)
      {
        count++;
      }
    }
    if(count==0)
    {
      printf("it is prime");
    }
    else
    {
      printf("it is not a prime");
    }
}
